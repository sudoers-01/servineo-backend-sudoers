{
  "compilerOptions": {
    "target": "ES6",
    "rootDir": "./src",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts", "src/modules/CRUD_operations/create_location.js", "src/models/Location.js", "src/database.js"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
